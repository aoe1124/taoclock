{
  "redirects": [
    {
      "source": "http://taoclock.cc/:path*",
      "destination": "https://www.taoclock.cc/:path*",
      "permanent": true
    },
    {
      "source": "https://taoclock.cc/:path*",
      "destination": "https://www.taoclock.cc/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        }
      ]
    }
  ]
} 